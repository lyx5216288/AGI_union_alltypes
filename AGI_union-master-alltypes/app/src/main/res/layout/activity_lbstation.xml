<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/orientation_find_layout_dividen_graph"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/orientation_find_result_list"
        android:layout_gravity="center">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000000" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:text="定位小区信息"
            android:gravity="center"
            app:layout_heightPercent="100%"/>


    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/cell_monitor_manual"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_below="@+id/cell_monitor_manual_title">

        <TextView
            android:id="@+id/cell_monitor_manual_earfcn_text"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_earfcn"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />

        <EditText
            android:id="@+id/lbs_manual_earfcn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_manual_earfcn_text"
            android:gravity="center"
            android:inputType="number"
            android:textSize="13sp"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />

        <TextView
            android:id="@+id/cell_monitor_manual_pci_text"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/lbs_manual_earfcn"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_pci"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />

        <EditText
            android:id="@+id/lbs_manual_pci"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:layout_toRightOf="@+id/cell_monitor_manual_pci_text"
            android:inputType="number"
            android:textSize="13sp"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />
        <Button
            android:layout_toRightOf="@+id/lbs_manual_pci"
            android:id="@+id/lbs_stop"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%"
            android:text="停止"
            android:visibility="visible"/>




    </android.support.percent.PercentRelativeLayout>
    <android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/cell_monitor_alarm_manual"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_below="@+id/cell_monitor_manual">

        <TextView
            android:id="@+id/cell_monitor_alarm_manual_tac_text"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_tac"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="10%" />

        <EditText
            android:id="@+id/lbs_alarm_manual_tac"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_alarm_manual_tac_text"
            android:gravity="center"
            android:inputType="number"
            android:textSize="13sp"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />

        <TextView
            android:id="@+id/cell_monitor_alarm_manual_ecgi_text"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/lbs_alarm_manual_tac"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_ecgi"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="15%" />

        <EditText
            android:id="@+id/lbs_alarm_manual_ecgi"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:layout_toRightOf="@+id/cell_monitor_alarm_manual_ecgi_text"
            android:inputType="number"
            android:textSize="13sp"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="35%" />
        <Button
            android:layout_toRightOf="@+id/lbs_alarm_manual_ecgi"
            android:id="@+id/lbs_location"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%"
            android:text="定位"/>


    </android.support.percent.PercentRelativeLayout>

    <LinearLayout
        android:layout_marginTop="5dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="RSRP:"/>
        <com.example.jbtang.agi_union.utils.MyProgress
            android:id="@+id/progressBar5"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            style="@style/myProgressBarStyle"
            android:max="120"
            android:progress="50"
            />
        </LinearLayout>

    <android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/cell_monitor_auto_cell_title"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/cell_monitor_alarm_manual">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#000000" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="附近小区列表"
            app:layout_heightPercent="100%" />
    </android.support.percent.PercentRelativeLayout>

    <android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/cell_monitor_auto_cell"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/cell_monitor_auto_cell_title">

        <TextView
            android:id="@+id/cell_monitor_list_earfcn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_earfcn"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />
        <TextView
            android:id="@+id/cell_monitor_list_pci"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_earfcn"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_pci"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="10%"/>
        <TextView
            android:id="@+id/cell_monitor_list_tai"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_pci"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_tac"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="15%" />

        <TextView
            android:id="@+id/cell_monitor_list_ecgi"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_tai"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_ecgi"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="20%" />

        <TextView
            android:id="@+id/cell_monitor_list_sinr"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_ecgi"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_sinr"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="10%" />

        <TextView
            android:id="@+id/cell_monitor_list_rsrp"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_sinr"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_item_rsrp"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="15%" />

        <TextView
            android:id="@+id/cell_monitor_list_choose"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_toRightOf="@+id/cell_monitor_list_rsrp"
            android:gravity="center"
            android:text="@string/page_test_cell_monitor_choose"
            app:layout_heightPercent="100%"
            app:layout_widthPercent="10%" />

    </android.support.percent.PercentRelativeLayout>

    <ListView
        android:id="@+id/lbstation_listView"
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:layout_below="@+id/cell_monitor_auto_cell"/>


    <!--<LinearLayout-->
    <!--style="?android:attr/progressBarStyleHorizontal"-->

    <!--android:id="@+id/lbstation_find_layout_result_graph"-->
        <!--android:layout_width="fill_parent"-->
        <!--android:layout_height="fill_parent"-->
        <!--android:orientation="horizontal" />-->
    <LinearLayout
        android:id="@+id/lbstation_find_layout_cell_status_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">
    </LinearLayout>



</LinearLayout>